{
  "name": "@bwp-web/canvas",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "clean": "rm -rf dist node_modules .turbo",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fabric": "^7.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@bwp-web/eslint-config": "*",
    "@types/react": "^19.1.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2"
  }
}
